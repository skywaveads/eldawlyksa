# Environment Variables for Nabaa Al-Khaleej Website
# For Vercel deployment
# Copy these variables to your Vercel project environment variables

# Site configuration
NEXT_PUBLIC_SITE_URL=https://nabaaalkhaleej.com
NEXT_PUBLIC_SITE_NAME=Nabaa Al-Khaleej
NEXT_PUBLIC_SITE_DESCRIPTION="Servicio de transporte refrigerado en Arabia Saudita"

# API Keys
INDEXNOW_KEY=YOUR_INDEXNOW_KEY_HERE

# Analytics (if applicable)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# API endpoints (if applicable)
NEXT_PUBLIC_API_URL=https://api.nabaaalkhaleej.com

# Feature flags
NEXT_PUBLIC_ENABLE_BLOG=true
NEXT_PUBLIC_ENABLE_TRACKING=true

# Contact form (if applicable)
EMAIL_SERVICE=
EMAIL_USER=
EMAIL_PASS=
CONTACT_EMAIL=contact@nabaaalkhaleej.com 